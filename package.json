{
  "name": "@coti/pod-sdk",
  "version": "0.1.0",
  "description": "Coti PoD SDK - Solidity contracts and TypeScript utilities for PoD dApps",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
