  },
  "files": [
    "dist",
    "contracts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "coti",
    "pod",
    "sdk",
    "solidity",
    "mpc"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@coti-io/coti-sdk-typescript": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
